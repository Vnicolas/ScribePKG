
    WSGIDaemonProcess scribepkg user=admin group=DomainAdmins threads=5
    WSGIScriptAlias /scribepkg /var/www/html/scribepkg/scribepkg.wsgi

    <Directory /var/www/html/scribepkg>
        WSGIProcessGroup scribepkg
        WSGIApplicationGroup %{GLOBAL}
        Order deny,allow
        Allow from all
    </Directory>
