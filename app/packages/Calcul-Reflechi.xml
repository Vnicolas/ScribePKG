<?xml version="1.0" encoding="UTF-8"?>
<packages>
<package
    id="Calcul-Reflechi"
    name="Calcul Reflechi - Logiciel leterrier"
    revision="%version%"
    reboot="false"
    priority="100">
    
    <variable name="version" value="0.0.3" />
    
    <depends package-id="Tcl-Tk"/>
    
    <check type="uninstall" condition="exists" path="Le Terrier d'AbulEdu - Calcul Réfléchi %version%" />
    
    <install cmd='"%SOFTWARE%\leterrier\leterrier-calc-ref-%version%-setup.exe" /VERYSILENT' />
    
    <upgrade cmd='"%SOFTWARE%\leterrier\leterrier-calc-ref-%version%-setup.exe" /VERYSILENT' />
    
    <remove cmd='"%PROGRAMFILES%\AbulEdu\Calcul_reflechi\unins000.exe" /SILENT' />


</package>
</packages>
