<?xml version="1.0" encoding="UTF-8"?>
<packages>
<package
    id="Wpkg-remove-wpkg_xml"
    name="Supprime WPKG.xml"
    revision="1"
    priority="9999"
    reboot="false"
    execute="once">
    

    <install cmd='%COMSPEC% /C DEL /F /Q "%WINDIR%\System32\wpkg.xml"' />
    <upgrade include='install' />
    <downgrade include='install' />
    <remove cmd='echo "nothing"' />

</package>
</packages>
